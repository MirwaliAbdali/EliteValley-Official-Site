{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "jsx-a11y/img-redundant-alt": "warn", // Change from "error" to "warn"
    "jsx-a11y/anchor-is-valid": "warn" // Change from "error" to "warn"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
